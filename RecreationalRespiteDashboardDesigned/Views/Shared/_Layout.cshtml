<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    <link href="~/Content/styleMenuBar.css" rel="stylesheet" />
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.1.1/jquery.min.js"></script>
    <link href="//netdna.bootstrapcdn.com/font-awesome/3.2.1/css/font-awesome.css" rel="stylesheet">
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <div class="container">
        <div class="row">
          

            <div id="wrapper">


                <!-- Sidebar -->
                <div id="sidebar-wrapper">
                    <ul class="sidebar-nav" style="margin-left:0;">
                        <li class="sidebar-brand">

                            <a href="#menu-toggle" id="menu-toggle" style="margin-top:20px;float:right;">
                                <i class="fa fa-bars " style="font-size:20px !Important" aria-hidden="true"></i>

                            </a>
                        <li>
                            <a href="http://localhost:58965/articles"><i class="fa fa-sort-alpha-asc " aria-hidden="true"> </i> <span style="margin-left:10px;"> Articles</span>  </a>
                        </li>
                        <li>
                            <a href="http://localhost:58965/events"> <i class="fa fa-play-circle-o " aria-hidden="true"> </i> <span style="margin-left:10px;"> Events</span> </a>
                        </li>
                        <li>
                            <a href="http://localhost:58965/diagnoses"> <i class="fa fa-puzzle-piece" aria-hidden="true"> </i> <span style="margin-left:10px;"> Diagnosis</span> </a>
                        </li>
                        <li>
                            <a href="http://localhost:58965/participants"> <i class="fa fa-font" aria-hidden="true"> </i> <span style="margin-left:10px;"> Participants</span> </a>
                        </li>
                        <li>
                            <a href="http://localhost:58965/userInformation"><i class="fa fa-info-circle " aria-hidden="true"> </i> <span style="margin-left:10px;"> Users </span> </a>
                        </li>
                        <li>
                            <a href="http://localhost:58965/regions"> <i class="fa fa-comment-o" aria-hidden="true"> </i> <span style="margin-left:10px;"> Regions</span> </a>
                        </li>
                    </ul>
                </div>
                <!-- /#sidebar-wrapper -->
                <!-- Page Content -->
                <div id="page-content-wrapper">
                    <div class="container-fluid">
                        <div class="row">
                            <div class="col-lg-12">



</div>
                            </div>


                                <!-- /#page-content-wrapper -->
                                <!-- /#wrapper -->



                            </div>
                        </div>
                        <div class="container body-content">
                            @RenderBody()
                            <hr />
                            <footer>
                                <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
                            </footer>
                        </div>

                        @Scripts.Render("~/bundles/jquery")
                        @Scripts.Render("~/bundles/bootstrap")
                        @RenderSection("scripts", required: false)

                <script>
    $("#menu-toggle").click(function(e) {
        e.preventDefault();
        $("#wrapper").toggleClass("toggled");
    });
    </script>
                </div></div>
        </div>

</body>
</html>
